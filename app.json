{
  "expo": {
    "name": "StudyPal",
    "slug": "StudyPal",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/blob.png",
    "scheme": "studypal",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/imageDesign.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/imageDesignn.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/imageDesign.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            {
              "asset": "./assets/fonts/Raleway-Medium.ttf"
            },
            {
              "asset": "./assets/fonts/Raleway-Regular.ttf"
            },
            {
              "asset": "./assets/fonts/Raleway-SemiBold.ttf"
            }
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
